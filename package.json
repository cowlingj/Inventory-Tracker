        "@babel/plugin-transform-async-to-generator": "^7.4.0",
        "@babel/polyfill": "^7.4.3",
        "@babel/preset-env": "^7.4.3",
        "babel-loader": "^8.0.5",
        "babel-plugin-syntax-async-functions": "^6.13.0",
        "core-js": "^3.0.1",
        "filemanager-webpack-plugin": "^2.0.5",
        "prettier": "^1.17.0",
        "webpack": "^4.29.6",
        "webpack-cli": "^3.3.0"
    }
